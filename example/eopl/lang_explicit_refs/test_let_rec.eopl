let p = 9
let devil = 666
let angel = 123
let true = zero_p(0)
let false = zero_p(1)
let rec odd = (x) => {
  if zero_p(x) {
    false
  } else {
    if zero_p(diff(x, 1)) {
      true
    } else {
      odd(diff(x, 2))
    }
  }
}
let rec even = (x) => {
  if zero_p(x) {
    true
  } else {
    if zero_p(diff(x, 1)) {
      false
    } else {
      even(diff(x, 2))
    }
  }
}
if even(p) {
  devil
} else {
  angel
}
