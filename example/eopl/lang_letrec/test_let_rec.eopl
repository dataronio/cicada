let p = 9 in
let devil = 666 in
let angel = 123 in
let true = zero_p(0) in
let false = zero_p(1) in
let rec odd = (x) => {
  if zero_p(x) {
    false
  } else {
    if zero_p(diff(x, 1)) {
      true
    } else {
      odd(diff(x, 2))
    }
  }
} in
let rec even = (x) => {
  if zero_p(x) {
    true
  } else {
    if zero_p(diff(x, 1)) {
      false
    } else {
      even(diff(x, 2))
    }
  }
} in
if even(p) {
  devil
} else {
  angel
}
